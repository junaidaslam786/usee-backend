{
  "name": "usee360-backend",
  "version": "1.0.0",
  "description": "A boilerplate for Node.js api projects",
  "license": "MIT",
  "author": {
    "name": "Ali Haider",
    "email": "a.haider_5@hotmail.com"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "node ./dist/bin/www.js",
    "start:dev": "nodemon --ignore dist/ --ignore test/ --exec babel-node ./src/bin/www.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "NODE_ENV=test mocha --require @babel/register --exit",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:create-migration": "npx sequelize-cli migration:create --name $1",
    "db:rollback": "npx sequelize-cli db:migrate:undo"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.41",
    "@sentry/node": "^6.7.2",
    "@sentry/tracing": "^6.7.2",
    "@stripe/stripe-js": "^2.1.11",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-validator": "^6.4.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "8.5.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.16",
    "opentok": "^2.16.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "redis": "^3.1.1",
    "sequelize": "^6.20.1",
    "stripe": "^13.11.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@types/ejs": "^3.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.3",
    "sequelize-cli": "^6.4.1"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  }
}
